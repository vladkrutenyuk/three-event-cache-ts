{
  "name": "@vladkrutenyuk/event-cache",
  "version": "0.1.1",
  "description": "Utility for THREE.EventDispatcher to cache event data easy and neat, reducing object creation overhead. One-line get and update cached data. TypeScript support and automatic EventMap inference to keep single source of trust.",
  "homepage": "https://github.com/vladkrutenyuk/three-event-cache-ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/vladkrutenyuk/three-event-cache-ts"
  },
  "type": "module",
  "main": "./dist/eventcache.esm.min.js",
  "module": "./dist/eventcache.esm.min.js",
  "types": "./dist/eventcache.d.ts",
  "exports": {
    ".": {
      "import": "./dist/eventcache.esm.min.js",
      "types": "./dist/eventcache.d.ts"
    }
  },
  "scripts": {
    "rbuild": "rimraf ./dist && rollup -c",
    "test": "jest",
    "build": "npm run test && npm run rbuild"
  },
