    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.14",
    "@types/three": "^0.164.1",
    "three": "^0.164.1",
    "buffer": "^5.5.0||^6.0.0",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
